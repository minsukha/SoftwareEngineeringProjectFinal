<% for(var i=0; i < nameArray.length; i++) { %>
   <tr>
     <td><%= jerseyArray[i] %></td>
     <td><form action="/getUserProfile" method="post"><input type="text" id="userId" name="userId" value="<%= memberIdArray[i] %>" hidden><a href="#" onclick="document.forms[0].submit();return false;"><%= nameArray[i] %></a></form></td>
